{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  customDatasources: {
    'aks': {
      defaultRegistryUrlTemplate: 'https://aksver.k8sj.io/eastus',
      format: 'json',
    },
  },
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/.+\\.tf$/',
      ],
      matchStrings: [
        '# renovate:\\s*depName=(?<packageName>[^\\s]+)\\s*datasource=(?<datasource>[^\\s]+)(?:\\s*registryUrl=(?<registryUrl>[^\\s]+))?\\s*\\n\\s*(?<terraformVar>\\w+)\\s*=\\s*"(?<currentValue>[^"]+)"',
      ],
      depNameTemplate: 'aks',
      datasourceTemplate: 'custom.aks',
      versioningTemplate: 'semver',
    },
  ],
}
